<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="1dp"
        android:top="1dp"
        android:left="1dp"
        android:right="1dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="3dp"
                android:color="#2B8FB6"
                />
            <solid android:color="#ffffff"/>
            <corners
                android:radius="20dp"/>
        </shape>
    </item>
</layer-list>