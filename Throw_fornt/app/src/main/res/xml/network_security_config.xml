<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
<base-config>
    <trust-anchors>
        <certificates src="@raw/cert"/>
        <certificates src="system"/>
    </trust-anchors>
</base-config>
<domain-config>
    <domain includeSubdomains="true">https://moviethree.synology.me</domain>
    <trust-anchors>
        <certificates src="@raw/cert"/>
    </trust-anchors>
</domain-config>
<domain-config cleartextTrafficPermitted="true">
    <domain includeSubdomains="true">maps.daum-img.net</domain>
</domain-config>
</network-security-config>